{
	"name": "dexbooru",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env.local -- vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"dbmigrate:dev": "dotenv -e .env.local -- npx prisma migrate dev",
		"dbpush:dev": "dotenv -e .env.local -- npx prisma db push",
		"dbstudio:dev": "dotenv -e .env.local -- npx prisma studio",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@types/bcryptjs": "^2.4.4",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"dotenv-cli": "^7.3.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.44.17",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "5.3.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^0.34.0"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "5.3.1",
		"aws-sdk": "^2.1467.0",
		"bcryptjs": "^2.4.3"
	}
}

{
	"name": "dexbooru",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env -- vite dev",
		"build": "prisma generate && vite build",
		"preview": "dotenv -e .env -- vite preview",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "prisma generate",
		"dbseed:dev": "dotenv -e .env -- prisma db push --force-reset && prisma db seed",
		"dbmigrate:dev": "dotenv -e .env -- prisma migrate dev",
		"dbreset:dev": "dotenv -e .env -- prisma migrate reset",
		"dbpush:dev": "dotenv -e .env -- prisma db push",
		"dbgenerate:dev": "dotenv -e .env -- prisma generate",
		"dbstudio:dev": "dotenv -e .env -- prisma studio",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/kit": "^2.7.3",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/markdown-it": "^14.1.2",
		"@types/markdown-it-emoji": "^3.0.1",
		"@types/node": "^20.17.1",
		"@types/nodemailer": "^6.4.16",
		"@types/qrcode": "^1.5.5",
		"@types/sanitize-html": "^2.13.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.20",
		"clsx": "^2.1.1",
		"csv-reader": "^1.0.12",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.46.0",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.44.24",
		"flowbite-svelte-icons": "^0.4.5",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.10",
		"postcss": "^8.4.47",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prisma": "^5.21.1",
		"svelte": "^5.1.3",
		"svelte-check": "^4.0.5",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.14",
		"tslib": "^2.8.0",
		"tsx": "^4.19.1",
		"typescript": "^5.6.3",
		"vite": "^5.4.10",
		"vitest": "^1.6.0"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.679.0",
		"@prisma/client": "^5.21.1",
		"@sveltejs/svelte-virtual-list": "^3.0.1",
		"@zerodevx/svelte-toast": "^0.9.6",
		"base32-encode": "^2.0.0",
		"bcryptjs": "^2.4.3",
		"jsonwebtoken": "^9.0.2",
		"markdown-it": "^14.1.0",
		"markdown-it-emoji": "^3.0.0",
		"nodemailer": "^6.9.15",
		"otpauth": "^9.3.4",
		"qrcode": "^1.5.4",
		"redis": "^4.7.0",
		"sanitize-html": "^2.13.1",
		"sharp": "^0.32.6",
		"svelte-tiny-virtual-list": "^2.1.2",
		"zod": "^3.23.8"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts --seed 69420 --user-count 20 --post-count 100 --tag-count 500 --artist-count 250 --comment-count 20000"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"packageManager": "pnpm@9.12.0",
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	}
}

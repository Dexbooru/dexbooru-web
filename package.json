{
	"name": "dexbooru-web",
	"version": "0.0.1",
	"author": "Tanish Shah",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "dotenv -e .env -o -- vite dev",
		"build": "prisma generate --schema ./prisma/schema && vite build",
		"preview": "dotenv -e .env -o -- vite preview",
		"run-build": "dotenv -e .env -o -- node build/index.js",
		"setup:localstack": "dotenv -e .env -o -- bash ./scripts/init-localstack.sh",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "svelte-kit sync && prisma generate",
		"dbpredeploy": "dotenv -e .env -o -- prisma migrate deploy",
		"dbseed": "dotenv -e .env -o -- prisma db push --force-reset && prisma db seed",
		"dbmigrate": "dotenv -e .env -o -- prisma migrate dev",
		"dbreset": "dotenv -e .env -o -- prisma migrate reset",
		"dbpush": "dotenv -e .env -o -- prisma db push",
		"dbgenerate": "dotenv -e .env -o -- prisma generate",
		"dbstudio": "dotenv -e .env -o -- prisma studio",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write 'src/**/*.{ts,svelte}'",
		"prepare": "husky install && svelte-kit sync"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.3",
		"@eslint/js": "^10.0.1",
		"@faker-js/faker": "^10.3.0",
		"@flowbite-svelte-plugins/chart": "^0.2.4",
		"@sveltejs/adapter-node": "^5.5.2",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/bcryptjs": "^3.0.0",
		"@types/html-to-text": "^9.0.4",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/markdown-it": "^14.1.2",
		"@types/markdown-it-emoji": "^3.0.1",
		"@types/minimist": "^1.2.5",
		"@types/node": "^25.2.2",
		"@types/nodemailer": "^7.0.9",
		"@types/nprogress": "^0.2.3",
		"@types/qrcode": "^1.5.6",
		"@types/sanitize-html": "^2.16.0",
		"@types/turndown": "^5.0.6",
		"@typescript-eslint/eslint-plugin": "^8.54.0",
		"@typescript-eslint/parser": "^8.54.0",
		"clsx": "^2.1.1",
		"csv-reader": "^1.0.12",
		"dotenv": "^17.2.4",
		"dotenv-cli": "^11.0.0",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"flowbite": "^4.0.1",
		"flowbite-svelte": "^1.31.0",
		"flowbite-svelte-icons": "^3.1.0",
		"globals": "^17.3.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"minimist": "^1.2.8",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"prisma": "^7.3.0",
		"svelte": "^5.50.0",
		"svelte-check": "^4.3.6",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.18",
		"tslib": "^2.8.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.985.0",
		"@aws-sdk/client-sqs": "^3.985.0",
		"@humanspeak/svelte-virtual-list": "^0.3.10",
		"@prisma/adapter-pg": "^7.3.0",
		"@prisma/client": "^7.3.0",
		"@zerodevx/svelte-toast": "^0.9.6",
		"base32-encode": "^2.0.0",
		"bcryptjs": "^3.0.3",
		"cookie": "^1.1.1",
		"html-to-text": "^9.0.5",
		"jdenticon": "^3.3.0",
		"jsonwebtoken": "^9.0.3",
		"markdown-it": "^14.1.0",
		"markdown-it-emoji": "^3.0.0",
		"nodemailer": "^8.0.1",
		"nprogress": "^0.2.0",
		"otpauth": "^9.5.0",
		"qrcode": "^1.5.4",
		"rabbitmq-client": "^5.0.8",
		"redis": "^5.10.0",
		"sanitize-html": "^2.17.0",
		"sharp": "^0.34.5",
		"turndown": "^7.2.2",
		"winston": "^3.19.0",
		"zod": "^4.3.6"
	},
	"peerDependencies": {
		"svelte": "^3.55.1 || ^4.0.0 || ^5.0.0 || ^5.41.0"
	},
	"lint-staged": {
		"src/**/*.{ts,svelte}": [
			"prettier --write"
		],
		"*.{js,jsx,ts,tsx,svelte}": [
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "pnpm@10.28.0",
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@prisma/engines",
			"prisma",
			"sharp",
			"esbuild"
		]
	}
}

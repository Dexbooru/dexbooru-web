{
	"name": "dexbooru",
	"version": "0.0.1",
	"author": "Tanish Shah",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "dotenv -e .env -o -- vite dev",
		"build": "prisma generate --schema ./prisma/schema && vite build",
		"preview": "dotenv -e .env -o -- vite preview",
		"run-build": "dotenv -e .env -o -- node build/index.js",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "prisma generate --schema ./prisma/schema",
		"dbpredeploy": "dotenv -e .env -o -- prisma migrate deploy",
		"dbseed": "dotenv -e .env -o -- prisma db push --force-reset && prisma db seed",
		"dbmigrate": "dotenv -e .env -o -- prisma migrate dev",
		"dbreset": "dotenv -e .env -o -- prisma migrate reset",
		"dbpush": "dotenv -e .env -o -- prisma db push --schema ./prisma/schema",
		"dbgenerate": "dotenv -e .env -o -- prisma generate --schema ./prisma/schema",
		"dbstudio": "dotenv -e .env -o -- prisma studio",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install && svelte-kit sync"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/html-to-text": "^9.0.4",
		"@types/jsonwebtoken": "^9.0.8",
		"@types/markdown-it": "^14.1.2",
		"@types/markdown-it-emoji": "^3.0.1",
		"@types/node": "^20.17.16",
		"@types/nodemailer": "^6.4.17",
		"@types/qrcode": "^1.5.5",
		"@types/sanitize-html": "^2.13.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.20",
		"clsx": "^2.1.1",
		"csv-reader": "^1.0.12",
		"dotenv-cli": "^7.4.4",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.46.1",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.44.24",
		"flowbite-svelte-icons": "^0.4.5",
		"husky": "^8.0.3",
		"lint-staged": "^15.4.2",
		"postcss": "^8.5.1",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prisma": "^6.2.1",
		"svelte": "^5.19.3",
		"svelte-check": "^4.1.4",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"vite": "^5.4.14",
		"vitest": "^1.6.0"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.735.0",
		"@prisma/client": "^6.2.1",
		"@zerodevx/svelte-toast": "^0.9.6",
		"base32-encode": "^2.0.0",
		"bcryptjs": "^2.4.3",
		"html-to-text": "^9.0.5",
		"jdenticon": "^3.3.0",
		"jsonwebtoken": "^9.0.2",
		"markdown-it": "^14.1.0",
		"markdown-it-emoji": "^3.0.0",
		"nodemailer": "^6.10.0",
		"otpauth": "^9.3.6",
		"qrcode": "^1.5.4",
		"redis": "^4.7.0",
		"sanitize-html": "^2.14.0",
		"sharp": "^0.32.6",
		"svelte-tiny-virtual-list": "^2.1.2",
		"winston": "^3.17.0",
		"zod": "^3.24.1"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts --seed 69420 --user-count 20 --post-count 100 --tag-count 500 --artist-count 250 --comment-count 20000"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "pnpm@9.12.0",
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	}
}

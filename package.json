{
	"name": "dexbooru-web",
	"version": "0.0.1",
	"author": "Tanish Shah",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "dotenv -e .env -o -- vite dev",
		"build": "prisma generate --schema ./prisma/schema && vite build",
		"preview": "dotenv -e .env -o -- vite preview",
		"run-build": "dotenv -e .env -o -- node build/index.js",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "prisma generate",
		"dbpredeploy": "dotenv -e .env -o -- prisma migrate deploy",
		"dbseed": "dotenv -e .env -o -- prisma db push --force-reset && prisma db seed",
		"dbmigrate": "dotenv -e .env -o -- prisma migrate dev",
		"dbreset": "dotenv -e .env -o -- prisma migrate reset",
		"dbpush": "dotenv -e .env -o -- prisma db push",
		"dbgenerate": "dotenv -e .env -o -- prisma generate",
		"dbstudio": "dotenv -e .env -o -- prisma studio",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "husky install && svelte-kit sync"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/bcryptjs": "^2.4.6",
		"@types/html-to-text": "^9.0.4",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/markdown-it": "^14.1.2",
		"@types/markdown-it-emoji": "^3.0.1",
		"@types/minimist": "^1.2.5",
		"@types/node": "^20.19.22",
		"@types/nodemailer": "^6.4.20",
		"@types/qrcode": "^1.5.5",
		"@types/sanitize-html": "^2.16.0",
		"@types/turndown": "^5.0.5",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.21",
		"clsx": "^2.1.1",
		"csv-reader": "^1.0.12",
		"dotenv-cli": "^7.4.4",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^8.10.2",
		"eslint-plugin-svelte": "^2.46.1",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.44.24",
		"flowbite-svelte-icons": "^0.4.5",
		"husky": "^8.0.3",
		"lint-staged": "^15.5.2",
		"minimist": "^1.2.8",
		"postcss": "^8.5.6",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prisma": "^6.17.1",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwind-merge": "^2.6.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.18",
		"tslib": "^2.8.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3",
		"vite": "^5.4.20",
		"vitest": "^1.6.1"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.913.0",
		"@prisma/client": "^6.17.1",
		"@zerodevx/svelte-toast": "^0.9.6",
		"base32-encode": "^2.0.0",
		"bcryptjs": "^2.4.3",
		"html-to-text": "^9.0.5",
		"jdenticon": "^3.3.0",
		"jsonwebtoken": "^9.0.2",
		"markdown-it": "^14.1.0",
		"markdown-it-emoji": "^3.0.0",
		"nodemailer": "^6.10.1",
		"otpauth": "^9.4.1",
		"qrcode": "^1.5.4",
		"redis": "^4.7.1",
		"sanitize-html": "^2.17.0",
		"sharp": "^0.32.6",
		"svelte-tiny-virtual-list": "^2.1.2",
		"turndown": "^7.2.1",
		"winston": "^3.18.3",
		"zod": "^3.25.76"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		]
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"packageManager": "pnpm@9.12.0",
	"exports": {
		".": {
			"svelte": "./dist/index.js"
		}
	}
}
